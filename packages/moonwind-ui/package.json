{
  "name": "moonwind-ui",
  "version": "1.0.0",
  "description": "A modern Vue 3 component library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./style": {
      "import": "./dist/style.js",
      "require": "./dist/style.cjs",
      "types": "./dist/style.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui",
    "typescript",
    "moonwind",
    "fengyue"
  ],
  "author": "fengyue",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/windlikeyou/moonwind-ui.git"
  },
  "homepage": "https://windlikeyou.github.io/moonwind-ui/",
  "bugs": {
    "url": "https://github.com/windlikeyou/moonwind-ui/issues"
  },
  "peerDependencies": {
    "ag-grid-community": "^34.0.0",
    "ag-grid-vue3": "^34.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "concurrently": "^9.2.1",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.9.2",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^2.2.12"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "dependencies": {
    "@moonwind-ui/components": "workspace:*",
    "@moonwind-ui/hooks": "workspace:*",
    "@moonwind-ui/styles": "workspace:*",
    "@moonwind-ui/utils": "workspace:*"
  }
}